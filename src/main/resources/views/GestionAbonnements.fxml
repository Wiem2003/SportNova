<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="gestionAbonnement" prefHeight="350.0" prefWidth="468.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/23.0.1" fx:controller="controllers.AbonnementController">
    <!-- Bouton pour créer un abonnement -->
    <Button fx:id="creerAbonnementButton" layoutX="85.0" layoutY="162.0" onAction="#handleCreerAbonnementAction" text="Créer Abonnement" />
    <!-- Bouton pour voir les abonnements existants -->
    <Button fx:id="abonnementsExistantsButton" layoutX="262.0" layoutY="162.0" onAction="#handleAbonnementsExistantsAction" text="Abonnements Existants" />
    <Button fx:id="retourButton" layoutX="771.0" layoutY="570.0" onAction="#handleRetour" text="Retour" />
</AnchorPane>
