<BorderPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.AbonnementsExistantsController">
    <center>
        <AnchorPane>
            <ListView fx:id="abonnementsList" layoutX="20.0" layoutY="20.0" prefHeight="300.0" prefWidth="550.0" />
            <Button fx:id="modifierSupprimerButton" layoutX="20.0" layoutY="330.0" onAction="#handleModifierSupprimerAction" text="Modifier / Supprimer" />
            <Button fx:id="retourButton" layoutX="771.0" layoutY="570.0" onAction="#handleRetour" text="Retour" />
        </AnchorPane>
    </center>
</BorderPane>
